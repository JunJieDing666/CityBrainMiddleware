<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.djj.middleware.entities.DeviceEntity" table="device" schema="citybrainmiddleware">
        <id name="id" column="Id"/>
        <property name="deviceNo" column="DeviceNo"/>
        <property name="interSectionId" column="InterSectionID"/>
        <property name="deviceIp" column="DeviceIP"/>
        <property name="devicePort" column="DevicePort"/>
        <property name="dir" column="Dir"/>
    </class>
</hibernate-mapping>