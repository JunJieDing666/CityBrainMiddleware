<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.djj.middleware.entities.EvaluationListEntity" table="evaluation_list"
           schema="citybrainmiddleware">
        <id name="evaluationId" column="Evaluation_Id"/>
        <property name="id" column="Id"/>
        <property name="laneNo" column="LaneNo"/>
        <property name="stops" column="Stops"/>
        <property name="delay" column="Delay"/>
        <property name="fuel" column="Fuel"/>
        <property name="emissions" column="Emissions"/>
    </class>
</hibernate-mapping>